zad 3
a)
update postac set statek = 'Statek Bjorna' where nazwa LIKE '%a%';
b)
update statek set max_ladownosc=max_ladownosc*0.7
where data_wodowania >= '1901-01-01'
and data_wodowania <= '2000-12-31';
